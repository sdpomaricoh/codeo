{
    "env": {
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "accessor-pairs": 0,
        "block-scoped-var": 2,
        "brace-style": [2, 1tbs],
        "comma-dangle": [2, never],
        "complexity": [2, 8],
        "dot-location": [2, property],
        "dot-notation": 2,
        "eol-last": 2,
        "eqeqeq": 0,
        "indent": [2, "tab"],
        "max-depth": [2, 6],
        "max-len": [2, 150, 4],
        "max-statements": [2, 40],
        "no-bitwise": 0,
        "no-dupe-class-members": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-eq-null": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implied-eval": 2,
        "no-inner-declarations": [2, "both"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-negated-in-lhs": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-restricted-syntax": [2, WithStatement],
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "quotes": [2, single, avoid-escape],
        "semi": [2, "never"],
        "keyword-spacing": 2,
        "space-before-blocks": 2,
        "strict": [2, global],
        "use-isnan": 2,
        "valid-typeof": 2,
        "vars-on-top": 2
    }
}